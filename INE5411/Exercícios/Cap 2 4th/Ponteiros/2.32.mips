2.32 b)
void swap (int *p) {
	int temp;
	temp = *p;
	*p = *(p+1);
	*(p+1) = *p;
}

SWAP: 	lw $t0, 0($a0)
		lw $t1, 4($a0)
		sw $t0, 4($a0)
		sw $t1, 0($a0)
		jr $ra

