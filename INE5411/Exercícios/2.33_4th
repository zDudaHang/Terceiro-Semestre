int count(int a[], int n, int x) {
	int res = 0;
	int i;
	for( i = 0; i != n; i++) {
		if (a[i] == x) {
			res += 1;
		}
	}
	return res;
}

COUNT:
add $t0, $0, $0 # i = 0;
add $t1, $0, $0 # res = 0;
loop:	beq $t0, $a1, done
	sll $t2, $t0, 2 # t2 = 4i
	add $t3, $t2, $a0 # t3 = 4i + &a[0]
	lw $t2, 0 ($t3) # t2 = a[i]
	addi $t0, $t0, 1
	bne $t2, $a2, loop # se a[i] != x, volte para o loop
	addi $t1, $t1, 1
done:	add $v0, $t1, $0 
	jr $ra
