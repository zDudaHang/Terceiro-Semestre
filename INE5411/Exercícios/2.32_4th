void swap(int *p) {
	int temp;
	temp = *p;
	*p = *(p+1);
	*(p+1) = *p;
}

SWAP:	lw $t0, 0 ($a0) # temp = *p
	lw $t1, 4 ($a0) # t1 = *(p+1)
	sw $t1, 0 ($a0) # *p = *(p+1)
	sw $t0, 4 ($a0) # *(p+1) = temp
