	sll $a2, $a2, 2 # 4j
	add $t0, $a0, $a1 # v[k]
	add $t1, $a0, $a2 # v[j]
	lw $t2, 0 ($t0) # temp = v[k]
	lw $t3, 0 ($t1) # t3 = v[j]
	sw $t3, 0 ($t0) # v[k] = v[j]
	sw $t2, 0 ($t1) # v[j] = temp
	jr $ra


